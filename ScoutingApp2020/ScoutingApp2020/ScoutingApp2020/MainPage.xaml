<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            x:Class="ScoutingApp2020.MainPage"
            x:Name="MainTabbedPage"
            Title="Scouting App 2020"
            BackgroundColor="Black"
            BarTextColor="White"
            CurrentPageChanged="MainTabbedPage_CurrentPageChanged">

    <ContentPage Title="Start" Padding="10" TabIndex="0">
        <StackLayout>
            <!--App Heading-->
            <Label Text="Welcome to the Infinite Recharge Scouting App!"
                    Margin="10"
                    TextColor="White"
                    FontSize="35" 
                    HorizontalOptions="Start"
                    VerticalOptions="Start" />
            <StackLayout Orientation="Horizontal">
                <StackLayout Margin="10">

                    <!--Scout Name Text Field-->
                    <StackLayout Orientation="Horizontal">
                        <Label Text="Name:"
                           Margin="10"
                        TextColor="White"
                        FontSize="25"
                        HorizontalOptions="Start"
                        VerticalOptions="Center"/>
                        <Entry x:Name="ScoutName"
                        BackgroundColor="White"
                        FontSize="25"
                        WidthRequest="200"/>
                    </StackLayout>

                    <!--Match Number Text Field-->
                    <StackLayout Orientation="Horizontal">
                        <Label Text="Match Number:"
                           Margin="10"
                        TextColor="White"
                        FontSize="25"
                        HorizontalOptions="Start"
                        VerticalOptions="Center"/>
                        <Entry x:Name="MatchNumber"
                        BackgroundColor="White"
                        FontSize="25"
                        WidthRequest="75"
                        Keyboard="Numeric"
                        MaxLength="3"/>
                    </StackLayout>

                    <!--Replay Match Toggle-->
                    <StackLayout Orientation="Horizontal">
                        <Label Text="Replay Match?:"
                        Margin="10"
                        TextColor="White"
                        FontSize="25"
                        HorizontalOptions="Start"
                        VerticalOptions="Center"/>
                        <Switch x:Name="ReplayMatch"
                        Margin="10"
                        Scale="1"
                        OnColor="Lime"/>
                    </StackLayout>

                    <!--Team ID Text Field-->
                    <StackLayout Orientation="Horizontal">
                        <Label Text="Team #:"
                        Margin="10"
                        TextColor="White"
                        FontSize="25"
                        HorizontalOptions="Start"
                        VerticalOptions="Center"/>
                        <Entry x:Name="TeamNumber"
                        BackgroundColor="White"
                        FontSize="25"
                        WidthRequest="130"
                        Keyboard="Numeric"
                        MaxLength="4"/>
                    </StackLayout>
                    
                    <!--Alliance Color Text Field-->
                    <StackLayout Orientation="Horizontal">
                        <Label Text="Alliance Color:"
                        Margin="10"
                        TextColor="White"
                        FontSize="25"
                        HorizontalOptions="Start"
                        VerticalOptions="Center"/>

                        <Picker x:Name="AllianceColorPicker" WidthRequest="100" VerticalOptions="Center" FontSize="24" TextColor="Black" BackgroundColor="White">

                        

                            <Picker.Items>
                                <x:String>Blue</x:String>
                                <x:String>Red</x:String>
                            </Picker.Items>
                        </Picker>
                    </StackLayout>

                    <!--Start Position Text Field-->
                    <StackLayout Orientation="Horizontal">
                        <Label Text="Start Position:"
                        Margin="10"
                        TextColor="White"
                        FontSize="25"
                        HorizontalOptions="Start"
                        VerticalOptions="Center"/>
                        <Entry x:Name="StartPosition"
                        BackgroundColor="White"
                        FontSize="25"
                        WidthRequest="75"
                        Keyboard="Numeric"
                        MaxLength="1"/>
                    </StackLayout>

                    <!--Num Balls Preloaded Text Field-->
                    <StackLayout Orientation="Horizontal">
                        <Label Text="Number Preloads:"
                        Margin="10"
                        TextColor="White"
                        FontSize="25"
                        HorizontalOptions="Start"
                        VerticalOptions="Center"/>
                        <Entry x:Name="Preloaded"
                        BackgroundColor="White"
                        FontSize="25"
                        WidthRequest="60"
                        Keyboard="Numeric"
                        MaxLength="1"/>

                    </StackLayout>
                    <Image Source="/Resources/drawable/logo.png" HorizontalOptions="Start" WidthRequest="300"/>
                </StackLayout>



                <StackLayout Margin="10">
                    <Image Source="/Resources/drawable/fieldGrid.PNG" 
                    HorizontalOptions="Start" 
                    WidthRequest="900" 
                     VerticalOptions="Center"/>

                    <Label Text="*IMPORTANT*: If the robot is placed exactly over the center line, input either 10, 30, or 50 as the start position. Have a fantastic day."
                       TextColor="White"
                       FontSize="25"/>
                </StackLayout>
            </StackLayout>
        </StackLayout>

    </ContentPage>

    <ContentPage Title="Auto" Padding="10" TabIndex="1">
        <!-- Initiation is Aqua (i like aqua) and buttons are brown (auto tab is brown) you are welcome to change anything you like -->

        <!-- Entire Page H -->
        <StackLayout Orientation="Horizontal">

            <!-- Left Buttons (Power Cells) -->
            <StackLayout Orientation="Vertical">
                <Label Margin="10"/>
            </StackLayout>

            <StackLayout Orientation="Vertical">

                <!-- Spacing -->
                <StackLayout Orientation="Horizontal">
                    <Label/>
                </StackLayout>

                <!-- Initiation Check -->
                <StackLayout Orientation="Horizontal">
                    <Label Text="Initiation Line Crossed?" 
                       Margin="10"
                       TextColor="White"
                       FontSize="25"
                       HorizontalOptions="Start"
                       VerticalOptions="Center"/>
                    <Switch x:Name="InitLine"
                        Margin="10"
                        Scale="1.5"
                        OnColor="Aqua"/>
                </StackLayout>

                <!-- Spacing -->
                <StackLayout Orientation="Vertical" Spacing="10">
                    <Label/>
                    <Label/>
                </StackLayout>

                <!-- Auto: Inner Port score -->
                <StackLayout Orientation="Horizontal">
                    <Label Text="Inner Port  "
                       TextColor="White"
                       FontSize="35"
                       HorizontalOptions="Start"
                       VerticalOptions="Center"/>
                    <Button x:Name="AutoInnerMinus"                     
                        Text="-"
                        Font="50"
                        WidthRequest="100" 
                        HeightRequest="100" 
                        TextColor="Black" 
                        BorderColor="Brown" 
                        BorderWidth="4"/>
                    <Label Text="50" 
                       x:Name="AutoInner"  
                       FontSize="24"
                       HorizontalTextAlignment="Center"
                       VerticalTextAlignment="Center" 
                       TextColor="White"/>
                    <Button x:Name="AutoInnerPlus"
                        Text="+"
                        Font="50"
                        WidthRequest="100" 
                        HeightRequest="100"
                        TextColor="Black" 
                        BorderColor="Brown" 
                        BorderWidth="4"/>
                </StackLayout>

                <!-- Spacing -->
                <StackLayout Orientation="Vertical" Spacing="45">
                    <Label/>
                    <Label/>
                </StackLayout>

                <!-- Auto: Outer Port score -->
                <StackLayout Orientation="Horizontal">
                    <Label Text="Outer Port  "
                       TextColor="White"
                       FontSize="35"
                       HorizontalOptions="Start"
                       VerticalOptions="Center"/>
                    <Button x:Name="AutoOuterMinus"                     
                        Text="-"
                        Font="50"
                        WidthRequest="100" 
                        HeightRequest="100" 
                        TextColor="Black" 
                        BorderColor="Brown" 
                        BorderWidth="4"/>
                    <Label Text="50" 
                       x:Name="AutoOuter"  
                       FontSize="24"
                       HorizontalTextAlignment="Center"
                       VerticalTextAlignment="Center" 
                       TextColor="White"/>
                    <Button x:Name="AutoOuterPlus"                        
                        Text="+"
                        Font="50"
                        WidthRequest="100" 
                        HeightRequest="100"
                        TextColor="Black" 
                        BorderColor="Brown" 
                        BorderWidth="4"/>
                </StackLayout>

                <!-- Spacing -->
                <StackLayout Orientation="Vertical" Spacing="45">
                    <Label/>
                    <Label/>
                </StackLayout>

                <!-- Auto: Lower Port score -->
                <StackLayout Orientation="Horizontal">
                    <Label Text="Lower Port  "
                       TextColor="White"
                       FontSize="35"
                       HorizontalOptions="Start"
                       VerticalOptions="Center"/>
                    <Button x:Name="AutoLowerMinus"
                        Text="-"
                        Font="50"
                        WidthRequest="100" 
                        HeightRequest="100" 
                        TextColor="Black" 
                        BorderColor="Brown" 
                        BorderWidth="4"/>
                    <Label Text="50" 
                       x:Name="AutoLower"  
                       FontSize="24"
                       HorizontalTextAlignment="Center"
                       VerticalTextAlignment="Center" 
                       TextColor="White"/>
                    <Button x:Name="AutoLowerPlus"                        
                        Text="+"
                        Font="50"
                        WidthRequest="100" 
                        HeightRequest="100"
                        TextColor="Black" 
                        BorderColor="Brown" 
                        BorderWidth="4"/>
                </StackLayout>

            </StackLayout>

            <!-- Image w/ vert spacing -->
            <StackLayout Orientation="Vertical" Spacing="10">
                <Label/>
                <Image Source="/Resources/drawable/power_port.png"
                   HorizontalOptions="Start"
                   HeightRequest="620"
                   WidthRequest="400"/>
            </StackLayout>

            <!-- Spacing Image-Misc. -->
            <StackLayout Orientation="Vertical">
                <Label Margin="25"/>
            </StackLayout>

            <!-- Misc. Performance Right -->
            <StackLayout Orientation="Vertical">

                <!-- Spacing -->
                <StackLayout Orientation="Vertical" Spacing="50">
                    <Label/>
                    <Label/>
                </StackLayout>

                <!-- Missed Label -->
                <StackLayout Orientation="Horizontal">

                    <!-- Spacing -->
                    <StackLayout Orientation="Vertical">
                        <Label Margin="4"/>
                    </StackLayout>

                    <!-- Label -->
                    <StackLayout Orientation="Horizontal">
                        <Label Text="       Missed"
                           Margin="0"
                           TextColor="White"
                           FontSize="30"
                           HorizontalTextAlignment="Center"/>
                    </StackLayout>

                </StackLayout>

                <!-- Missed Buttons -->
                <StackLayout Orientation="Horizontal">
                    <Button x:Name="AutoMissedMinus"
                            Text="-"
                            Font="50"
                            WidthRequest="100"
                            HeightRequest="100"
                            TextColor="Black"
                            BorderColor="Brown"
                            BorderWidth="4" />
                    <Label Text="50"
                           x:Name="AutoMissed"
                           FontSize="24"
                           HorizontalTextAlignment="Center"
                           VerticalTextAlignment="Center"
                           TextColor="White" />
                    <Button x:Name="AutoMissedPlus"
                            Text="+"
                            Font="50"
                            WidthRequest="100"
                            HeightRequest="100"
                            TextColor="Black"
                            BorderColor="Brown"
                            BorderWidth="4" />
                </StackLayout>

                <!-- Spacing -->
                <StackLayout Orientation="Vertical">
                    <Label/>
                    <Label/>
                </StackLayout>

                <!-- Dropped Label -->
                <StackLayout Orientation="Horizontal">

                    <!-- Spacing -->
                    <StackLayout Orientation="Vertical"/>

                    <!-- Label -->
                    <StackLayout Orientation="Horizontal">
                        <Label Text="       Dropped"
                           Margin="0"
                           HorizontalOptions="End"
                           TextColor="White"
                           FontSize="30"/>
                    </StackLayout>

                </StackLayout>

                <!-- Dropped Buttons -->
                <StackLayout Orientation="Horizontal">
                    <Button x:Name="AutoDroppedMinus"
                            Text="-"
                            Font="50"
                            WidthRequest="100"
                            HeightRequest="100"
                            TextColor="Black"
                            BorderColor="Brown"
                            BorderWidth="4" />
                    <Label Text="50"
                           x:Name="AutoDropped"
                           FontSize="24"
                           HorizontalTextAlignment="Center"
                           VerticalTextAlignment="Center"
                           TextColor="White" />
                    <Button x:Name="AutoDroppedPlus"
                            Text="+"
                            Font="50"
                            WidthRequest="100"
                            HeightRequest="100"
                            TextColor="Black"
                            BorderColor="Brown"
                            BorderWidth="4" />
                </StackLayout>

                <!-- Spacing -->
                <StackLayout Orientation="Vertical">
                    <Label/>
                    <Label/>
                </StackLayout>

                <!-- Collected Label -->
                <StackLayout Orientation="Horizontal">
                    <Label Text="       Collected"
                           Margin="0"
                           HorizontalOptions="End"
                           TextColor="White"
                           FontSize="30"/>
                </StackLayout>

                <!-- Collected Buttons -->
                <StackLayout Orientation="Horizontal">
                    <Button x:Name="AutoCollectedMinus"
                            Text="-"
                            Font="50"
                            WidthRequest="100"
                            HeightRequest="100"
                            TextColor="Black"
                            BorderColor="Brown"
                            BorderWidth="4" />
                    <Label Text="50"
                           x:Name="AutoCollected"
                           FontSize="24"
                           HorizontalTextAlignment="Center"
                           VerticalTextAlignment="Center"
                           TextColor="White" />
                    <Button x:Name="AutoCollectedPlus"
                            Text="+"
                            Font="50"
                            WidthRequest="100"
                            HeightRequest="100"
                            TextColor="Black"
                            BorderColor="Brown"
                            BorderWidth="4" />
                </StackLayout>

            </StackLayout>

        </StackLayout>

    </ContentPage>
