    <ContentPage Title="EndGame" Padding="10" TabIndex="3">

        <StackLayout Orientation="Horizontal" Spacing="50">
            <StackLayout Orientation="Vertical" Spacing="50">
                <StackLayout Orientation="Horizontal">
                    <Image Source="Resources/drawable/endGame.png"
                           HorizontalOptions="Center"
                           WidthRequest="260" 
                           Margin="10"/>
                    <StackLayout Orientation="Vertical">
                        <StackLayout Orientation="Vertical">
                            <Label Text="Which Climb Zone Attempted?"
                                   TextColor="White"
                                   FontSize="24"
                                   VerticalOptions="Center" />
                            <Picker x:Name="ClimbZoneAttemptedPicker"
                                    WidthRequest="150"
                                    VerticalOptions="Center"
                                    FontSize="24"
                                    TextColor="Black"
                                    BackgroundColor="White">
                                <Picker.Items>
                                    <x:String>Did Not Climb</x:String>
                                    <x:String>Zone 1</x:String>
                                    <x:String>Zone 2</x:String>
                                    <x:String>Zone 3</x:String>
                                    <x:String>Zone 4</x:String>
                                </Picker.Items>
                            </Picker>
                            <Label Text="Which Climb Zone Achieved?"
                                   TextColor="White"
                                   FontSize="24"
                                   VerticalOptions="Center" />
                            <Picker x:Name="ClimbZoneAchievedPicker"
                                    WidthRequest="150"
                                    VerticalOptions="Center"
                                    FontSize="24"
                                    TextColor="Black"
                                    BackgroundColor="White">
                                <Picker.Items>
                                    <x:String>Did Not Climb</x:String>
                                    <x:String>Zone 1</x:String>
                                    <x:String>Zone 2</x:String>
                                    <x:String>Zone 3</x:String>
                                    <x:String>Zone 4</x:String>
                                </Picker.Items>
                            </Picker>
                            <Label Text="Bricked or Disabled?"
                                   TextColor="White"
                                   FontSize="24" />
                            <Picker x:Name="BreakdownPicker"
                                    WidthRequest="400"
                                    VerticalOptions="Center"
                                    FontSize="24"
                                    TextColor="Black"
                                    BackgroundColor="White">
                                <Picker.Items>
                                    <x:String>N/A</x:String>
                                    <x:String>Fell over and not righted</x:String>
                                    <x:String>Disconnected from field</x:String>
                                    <x:String>Massive mechanical failure</x:String>
                                    <x:String>Battery fell out/robot turned off</x:String>
                                    <x:String>Disabled for safety violation</x:String>
                                    <x:String>Bumpers fell off</x:String>
                                    <x:String>Other: talk about it in comments</x:String>
                                </Picker.Items>
                            </Picker>

                        </StackLayout>
                    </StackLayout>
                </StackLayout>
                <StackLayout Orientation="Horizontal">
                    <Label Text="Was Helped?"
                           TextColor="White"
                           FontSize="24" />
                    <Switch x:Name="EndHelped" />
                </StackLayout>
                <StackLayout Orientation="Horizontal">
                    <Label Text="Assisted Others?"
                           TextColor="White"
                           FontSize="24" />
                    <Switch x:Name="EndAssist" />
                </StackLayout>
                <StackLayout Orientation="Horizontal">
                    <StackLayout Orientation="Vertical">
                        <Label Text="Comments (include fouls)"
                               TextColor="White"
                               FontSize="24" />
                        <Entry x:Name="CommentsEntry"
                               FontSize="24"
                               Keyboard="Text"
                               WidthRequest="500"
                               TextColor="Black"
                               BackgroundColor="White" />
                    </StackLayout>
                </StackLayout>
                <StackLayout Orientation="Horizontal">
                    <Label Text="Start new file?"
                           VerticalOptions="Center"
                           TextColor="White"
                           FontSize="24" />
                    <Picker x:Name="NewFilePicker"
                            WidthRequest="100"
                            VerticalOptions="Center"
                            FontSize="24"
                            TextColor="Black"
                            BackgroundColor="White">
                        <Picker.Items>
                            <x:String>Yes</x:String>
                            <x:String>No</x:String>
                        </Picker.Items>
                    </Picker>
                    <Button x:Name="SubmitButton"
                            Text="Submit"
                            FontSize="24"
                            />
                </StackLayout>

            </StackLayout>
            <StackLayout Orientation="Vertical"
                         Spacing="25">
                <StackLayout Orientation="Horizontal"
                             Spacing="25">
                    <StackLayout Orientation="Vertical">
                        <Label Text="SOMETHING"
                               TextColor="White"
                               FontSize="24" />
                        <Picker x:Name="DefenseAmountPicker"
                                WidthRequest="100"
                                VerticalOptions="Center"
                                FontSize="24"
                                TextColor="Black"
                                BackgroundColor="White">
                            <Picker.Items>
                                <x:String>None</x:String>
                                <x:String>Some</x:String>
                                <x:String>A lot</x:String>
                            </Picker.Items>
                        </Picker>
                    </StackLayout>
                    <StackLayout Orientation="Vertical">
                        <Label Text="SOMETHING"
                               TextColor="White"
                               FontSize="24" />
                        <Picker x:Name="DefenseSkillPicker"
